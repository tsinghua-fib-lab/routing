  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Routing Profile",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/bin/routing_profile",
      "args": [
        "--flagfile",
        "flags/profile_beijing3.flag"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
      ]
    },
    {
      "name": "(gdb) Routing Mongo",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/bin/routing_server",
      "args": [
        "--flagfile",
        "flags/server_beijing3.flag"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
      ],
      "preLaunchTask": "CMake Compile"
    }
  ]
}
