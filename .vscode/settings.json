{
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "editor.tabSize": 4,
  "files.autoSave": "onFocusChange",
  "cSpell.words": [
    "ABSL",
    "aowenxuan",
    "Bson",
    "bsoncxx",
    "cmake",
    "conanbuildinfo",
    "conanfile",
    "cpprestsdk",
    "dylib",
    "EPSG",
    "flagfile",
    "gongyu",
    "grpc",
    "gtest",
    "libcurl",
    "librdkafka",
    "mongocxx",
    "protobuf",
    "protos",
    "SASL",
    "spdlog",
    "tsingroc",
    "Tsingroc",
    "wolong",
    "zhangjun"
  ],
  "editor.formatOnSave": true,
  "todohighlight.keywords": [
    {
      "text": "TODO",
      "color": "#fff",
      "backgroundColor": "#fdbc3eaa",
      "overviewRulerColor": "#fdbc3e",
      "isWholeLine": true,
    },
    {
      "text": "ATTENTION",
      "color": "#fff",
      "backgroundColor": "#f06292aa",
      "overviewRulerColor": "#f06292",
      "isWholeLine": true,
    }
  ],
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.showUnused": true,
}
